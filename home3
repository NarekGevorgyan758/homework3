<script>
	
  const z = function (point1, point2,) {
    if ( point1 === point2) {
      return 0 ;
    }
    else if 
      ( point1 > point2) {
      return 1 ; }
    
    else 
      (point1 < point2) {
        return -1 ;
      }
  }
      
 
</script>

